 @FindBy(className = "oxd-icon bi-caret-down-fill oxd-select-text--arrow")
 WebElement className1;
 
 @FindBy(className = "oxd-select-text oxd-select-text--active")
 WebElement className2;
 
 @FindBy(xpath="//*[@id=\"app\"]/div[1]/div[2]/div[2]/div/div/form/div[2]/div/div[1]/div/div[2]/div/div/i")
 WebElement xpath;
 
 @FindBy(xpath="/html/body/div/div[1]/div[2]/div[2]/div/div/form/div[2]/div/div[2]/div/div[2]/div/div/i")
 WebElement xpath1;
 
 @FindBy(className = "oxd-textarea oxd-textarea--active oxd-textarea--resize-vertical")
 WebElement className3;
 
 @FindBy(className = "oxd-button oxd-button--medium oxd-button--secondary orangehrm-left-space")
 WebElement className4;
 
 
 
 public void apply()
 {
	 linktext.click();
 }
 
